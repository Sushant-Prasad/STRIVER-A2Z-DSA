//link:-https://www.geeksforgeeks.org/problems/largest-element-in-array4009/1?page=1&sprint=bb436fe6ca33efc91d996a51a723a90a&sortBy=submissions

class Solution {
    public static int largest(int[] arr) {
        // code here
        int max = Integer.MIN_VALUE;
        int n = arr.length;
        for(int i =0;i<n;i++){
            max=Math.max(max,arr[i]);
        }
        return max;
    }
}
