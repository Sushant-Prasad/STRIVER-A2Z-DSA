class Solution {

    // Function to check whether a number is prime
    static boolean isPrime(int n) {
        
        // Edge case: numbers less than or equal to 1 are not prime
        if (n <= 1) return false;

        // Loop from 2 to âˆšn (i * i <= n)
        // If any number divides n, it's not prime
        for (int i = 2; i * i <= n; i++) {
            if (n % i == 0)
                return false;  // Found a divisor, so n is not prime
        }

        // No divisors found, hence n is prime
        return true;
    }

}
