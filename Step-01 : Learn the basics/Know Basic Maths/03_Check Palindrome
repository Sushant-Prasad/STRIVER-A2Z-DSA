//link:-https://leetcode.com/problems/palindrome-number/description/

class Solution {
    public boolean isPalindrome(int x) {
        int temp=x;
        int rev=0;
        if(x<0)return false;//for negative number
        while(x != 0){
           int digit = x%10;
           rev=rev*10 + digit;
           x=x/10; 
        }
        return (temp==rev);
        
    }
}
